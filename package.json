{
  "name": "@chameleoncloud/jupyterlab-theme-extension",
  "version": "0.0.5",
  "description": "JupyterLab - Chameleon Theme",
  "homepage": "https://github.com/chameleoncloud/jupyterlab-theme-extension",
  "bugs": {
    "url": "https://github.com/chameleoncloud/jupyterlab-theme-extension/issues"
  },
  "license": "BSD-3-Clause",
  "author": "University of Chicago",
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "static/*.css",
    "static/*.ttf",
    "static/*.eot",
    "static/*.woff",
    "static/*.woff2"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chameleoncloud/jupyterlab-theme-extension.git"
  },
  "scripts": {
    "build": "run-p build:**",
    "build:typescript": "tsc",
    "build:webpack": "webpack",
    "clean": "rimraf lib && rimraf static",
    "prepublishOnly": "npm run build",
    "watch": "tsc -b --watch",
    "watch:src": "tsc -b src --watch",
    "watch:webpack": "webpack --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.19.1",
    "@jupyterlab/apputils": "^0.19.1",
    "font-awesome": "~4.7.0"
  },
  "devDependencies": {
    "css-loader": "~0.28.7",
    "mini-css-extract-plugin": "^0.4.0",
    "npm-run-all": "~4.1.1",
    "rimraf": "~2.6.2",
    "svg-url-loader": "~2.3.1",
    "svgo": "~1.0.4",
    "svgo-loader": "~2.1.0",
    "typedoc": "~0.12.0",
    "typescript": "~3.1.1",
    "url-loader": "~1.0.1",
    "webpack": "~4.12.0",
    "webpack-cli": "^3.0.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "themeDir": "static"
  }
}
