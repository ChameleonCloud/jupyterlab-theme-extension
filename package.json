{
  "name": "@chameleoncloud/jupyterlab-theme-extension",
  "version": "1.0.0",
  "description": "JupyterLab - Chameleon Theme",
  "homepage": "https://github.com/chameleoncloud/jupyterlab-theme-extension",
  "bugs": {
    "url": "https://github.com/chameleoncloud/jupyterlab-theme-extension/issues"
  },
  "license": "BSD-3-Clause",
  "author": "University of Chicago",
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chameleoncloud/jupyterlab-theme-extension.git"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run build",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0",
    "@jupyterlab/apputils": "^1.0.0",
    "@jupyterlab/theme-light-extension": "^1.0.0"
  },
  "devDependencies": {
    "rimraf": "~2.6.2",
    "typedoc": "^0.14.2",
    "typescript": "~3.5.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "themePath": "style/index.css"
  }
}
